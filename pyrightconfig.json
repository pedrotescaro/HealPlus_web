{
  "typeCheckingMode": "basic",
  "pythonVersion": "3.13",
  "venvPath": ".venv",
  "reportMissingImports": false,
  "reportMissingTypeStubs": false,
  "reportOptionalMemberAccess": false,
  "reportOptionalSubscript": false,
  "reportGeneralTypeIssues": true,
  "ignore": [
    ".venv",
    "node_modules",
    "dist"
  ],
  "include": [
    "backend",
    "tests"
  ]
}
